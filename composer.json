  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "Doofinder\\": "src/",
      "Doofinder\\Search\\": "src/Search/",
      "Doofinder\\Management\\": "src/Management/",
      "DoofinderManagement\\": "src/Management/PhpClient/"
    }
